<project xmlns="http://maven.apache.org/DECORATION/1.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0
                             http://maven.apache.org/xsd/decoration-1.0.0.xsd"
         name="LensKit Recommender Implementation">
  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.3.0</version>
  </skin>
  <publishDate position="right"/>
  <version position="right"/>
  <poweredBy/>
  <bannerLeft>
    <name>LensKit</name>
    <src>logo.png</src>
    <href>http://lenskit.grouplens.org</href>
  </bannerLeft>
  <custom>
    <fluidoSkin>
      <gitHub>
        <projectId>grouplens/lenskit</projectId>
        <ribbonOrientation>right</ribbonOrientation>
        <ribbonColor>black</ribbonColor>
      </gitHub>
    </fluidoSkin>
  </custom>
  <body>
    <breadcrumbs>
      <item name="GroupLens Research"
            href="http://www.grouplens.org"/>
      <item name="LensKit"
            href="index.html"/>
    </breadcrumbs>
    <menu name="LensKit" inherit="top">
      <item name="Project Home" href="http://lenskit.grouplens.org"/>
      <item name="GitHub" href="http://github.com/grouplens/lenskit"/>
    </menu>
    <menu name="Documentation">
      <item name="Getting Started" href="http://github.com/grouplens/lenskit/wiki/GettingStarted"/>
      <item name="Manual" href="http://github.com/grouplens/lenskit/wiki/Manual"/>
      <item name="Release Notes" href="releases"/>
      <item name="JavaDoc" href="apidocs"/>
    </menu>
    <menu ref="modules"/>
    <menu name="Module Info" ref="reports" inherit="bottom"/>
  </body>
</project>
